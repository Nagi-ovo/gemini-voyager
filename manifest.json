{
  "manifest_version": 3,
  "name": "Gemini Voyager",
  "description": "Supercharge your Gemini experience with timeline navigation, folder organization, prompt library, and chat export.",
  "version": "1.0.1",
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "32": "icon-32.png"
    }
  },
  "options_ui": {
    "page": "src/pages/options/index.html",
    "open_in_tab": true
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*.googleusercontent.com/*",
    "https://lh3.google.com/*",
    "https://*.ggpht.com/*",
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; worker-src 'self'"
  },
  "background": {
    "service_worker": "src/pages/background/index.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/pages/content/index.tsx"
      ],
      "css": [
        "contentStyle.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png",
        "katex-config.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
